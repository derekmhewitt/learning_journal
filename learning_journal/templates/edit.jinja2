{% extends "layout.jinja2" %}
{% block body %}
            <h2>Edit an Existing Learning Journal Entry Here:</h2>
            <form action="" method="post">
                <input type="text" required>
                    {{ title }}
                </input>
                <textarea rows="20" cols="60" type="text" required>
                    {{ content }}
                </textarea>
                <input type="submit" value="submit" disabled>
            </form>
{% endblock %}